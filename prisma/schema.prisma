generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Livro {
  id            Int       @id @default(autoincrement())
  titulo        String    @unique
  autor         String
  saga          String
  subgenero     String
  paginas       Float
  anoPublicacao Float
  editora       String
  avaliacao     Float 

  @@map("livros")
}